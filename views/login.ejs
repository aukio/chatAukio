<div id="aukio-chat">
    		<div id="logo"><img  src="/Images/aukioLogo.png"></div>


      <% include ./partials/messages %>
      <form action="/users/login" method="POST">
          <label for="email">Email</label>
          <input 
            type="email"
            id="email"
            name="email"
            class="form-control"
            placeholder="Enter Email"
          />
          <label for="password">Password</label>
          <input
            type="password"
            id="password"
            name="password"
            class="form-control"
            placeholder="Enter Password"
          />
        <button class="normalButton" type="submit" >Login</button>
      </form>
      <div class="links">
      <p>
        <a>No account? </a><a href="/users/register">Register</a>
      </p>
      <p>
        <a href="/users/forgot">Forgot your password?</a>
      </p>
      </div>
    </div>
